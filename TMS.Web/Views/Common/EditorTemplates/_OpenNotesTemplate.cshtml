@model TMS.Library.Common.TMS_Notes
@using lr = Resources.Resources

<div class=" Alertbox alert alert-danger" style="display:none">
    <ul class="errors"></ul>
</div>
<div class="form-horizontal form-person-popup">
    <div class="form-group">
        @Html.LabelFor(model => model.NotesText, new { @class = "col-md-3 col-lg-3 control-label" })
        <div class="col-md-7 col-lg-7 col-sm-11">
            @(Html.TextAreaFor(c => c.NotesText, new { @class = "form-control ", style = "width:100%" }))
            @Html.ValidationMessageFor(model => model.NotesText)
        </div>
    </div>
</div>

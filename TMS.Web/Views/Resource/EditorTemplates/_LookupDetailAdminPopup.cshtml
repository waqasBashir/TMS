@model TMS.Library.Admin.Lookup.LookupDetail
<div class="k-edit-form-container">
    <div class=" Alertbox alert alert-danger" style="display:none">
        <ul class="errors"></ul>

    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.LookupDetailP_Name)
    </div>
    <div class="editor-field">
        @Html.Kendo().TextBoxFor(model => model.LookupDetailP_Name).HtmlAttributes(new { @style = "width:100%" })
        @Html.ValidationMessageFor(model => model.LookupDetailP_Name)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.LookupDetailS_Name)
    </div>
    <div class="editor-field">
        @Html.Kendo().TextBoxFor(model => model.LookupDetailS_Name).HtmlAttributes(new { @style = "width:100%" })
        @Html.ValidationMessageFor(model => model.LookupDetailS_Name)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.IsActive)
    </div>
    <div class="editor-field">
        @Html.Kendo().CheckBoxFor(model => model.IsActive)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.IsSelected)
    </div>
    <div class="editor-field">
        @Html.Kendo().CheckBoxFor(model => model.IsSelected)
    </div>

</div>

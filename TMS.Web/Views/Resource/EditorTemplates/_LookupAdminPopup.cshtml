@model TMS.Library.Admin.Lookup.Lookup
<div class="k-edit-form-container">
    <div class=" Alertbox alert alert-danger" style="display:none">
        <ul class="errors"></ul>

    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.LookupP_Name)
    </div>
    <div class="editor-field">
        @Html.Kendo().TextBoxFor(model => model.LookupP_Name).HtmlAttributes(new { @style = "width:100%" })
        @Html.ValidationMessageFor(model => model.LookupP_Name)
    </div>
    <div class="editor-label">
        @Html.LabelFor(model => model.LookupS_Name)
    </div>
    <div class="editor-field">
        @Html.Kendo().TextBoxFor(model => model.LookupS_Name).HtmlAttributes(new { @style = "width:100%" })
        @Html.ValidationMessageFor(model => model.LookupS_Name)
    </div>
    
</div>
